{
    "plugins": [
        "promise",
        "prettier"
    ],
    "extends": [
        "airbnb-base",
        "prettier"
    ],
    "env": {
        "node": true,
        "es2021": true
    },
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "prettier/prettier": "error",
        "no-underscore-dangle": "off",
        "import/extensions": "off",
        "no-console": "off",
        "consistent-return": "warn",
        "promise/catch-or-return": "error",
        "eqeqeq": "error"
    }
}